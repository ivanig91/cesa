<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.control.Button?>
<GridPane  hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProveedorDetalleController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label style="-fx-font: NORMAL 20 Tahoma;" text="Proveedor" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <padding>
            <Insets   right="10.0"  />
        </padding>
    </Label>



    <GridPane GridPane.columnIndex="0" GridPane.rowIndex="2">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Label  style="-fx-font: NORMAL 15 Tahoma;" text="CIF Proveedor" GridPane.columnIndex="0" GridPane.rowIndex="0" >
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <Label style="-fx-font: NORMAL 15 Tahoma;" text="Razón Social" GridPane.columnIndex="0" GridPane.rowIndex="1" >
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <Label style="-fx-font: NORMAL 15 Tahoma;" text="Número de Registro Notarial" GridPane.columnIndex="0" GridPane.rowIndex="2" >
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <Label style="-fx-font: NORMAL 15 Tahoma;" text="Código de Seguro de Responsabilidad Civil" GridPane.columnIndex="0" GridPane.rowIndex="3">
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <Label style="-fx-font: NORMAL 15 Tahoma;" text="Importe del Seguro de Responsabilidad Civil" GridPane.columnIndex="0" GridPane.rowIndex="4" >
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <Label style="-fx-font: NORMAL 15 Tahoma;" text="Fecha de Homologación" GridPane.columnIndex="0" GridPane.rowIndex="5" >
            <padding>
                <Insets bottom="10.0"  right="10.0" top="10.0" />
            </padding>
        </Label>
        <TextField fx:id="cifProveedor" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <TextField fx:id="razSocial" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="regNotarial" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="codSRC" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <TextField fx:id="importeSRC" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <TextField fx:id="fecHom" style="-fx-font: NORMAL 15 Tahoma;" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <Button fx:id="btAccion" mnemonicParsing="false" onMouseClicked="#accionButton" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="6" textAlignment="JUSTIFY"  >
            <padding>
                <Insets bottom="10.0"  right="10.0" left="10.0" top="10.0" />
            </padding>
        </Button>

      <columnConstraints>
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
      </rowConstraints>

        
    </GridPane>

    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
</GridPane>
